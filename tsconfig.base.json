{
  "compilerOptions": {
      "target": "ES2022",
      "moduleDetection": "force",
      "esModuleInterop": true,
      "verbatimModuleSyntax": true,
      "isolatedModules": true,
      "resolveJsonModule": true,
      "skipLibCheck": true,

      "strict": true,
      "noUncheckedIndexedAccess": true,
      "noImplicitOverride": true,

      "module": "Preserve",
      "noEmit": true,
      
      "lib": [
          "es2022"
      ]
  }
}